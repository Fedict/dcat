    <http://www.w3.org/ns/dcat#keyword> "speelweefselplan"@nl, "jeugd"@nl, "veiligheid"@nl, "wandel"@nl, "kinderen"@nl, "mobiliteit"@nl, "fiets"@nl, "routes"@nl;
