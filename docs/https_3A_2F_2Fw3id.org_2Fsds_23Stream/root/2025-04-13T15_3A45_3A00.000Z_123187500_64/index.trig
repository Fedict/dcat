    <http://www.w3.org/ns/dcat#keyword> "ruimtelijke beleidsplannen"@nl, "ruimtelijk beleid"@nl, "visie"@nl, "srsa selectiekaarten vector"@nl, "Antwerpen Ontwerpen"@nl, "ruimtelijke ordening"@nl, "structuurplan"@nl, "strategische kaart"@nl, "selectiekaart"@nl, "RO"@nl, "sRSA"@nl, "ruimtelijke planning"@nl;
