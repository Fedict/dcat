    <http://www.w3.org/ns/dcat#keyword> "MercatorNet"@nl, "OpenbareMetadata"@nl, "bodemafdekking"@nl, "verharding"@nl, "ruimtemonitor"@nl, "lc_jaarbak_5m_2016"@nl, "Jaarlijkse bodemafdekkingskaart (JaarBAK), 5 m-resolutie, 2016"@nl;
    <http://purl.org/dc/terms/subject> <https://metadata.vlaanderen.be/id/GDI-Vlaanderen-Vlaamse-Administratieve-Eenheden/Vlaams%20Gewest>, <http://inspire.ec.europa.eu/metadata-codelist/SpatialScope/regional>;
