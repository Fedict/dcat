    <http://www.w3.org/ns/dcat#keyword> "Checklist"@nl, "parakeet"@nl, "parrot"@nl, "Turkish parakeet census project"@nl, "invasive species"@nl, "Turkey"@nl, "Inventorythematic"@nl, "alien species"@nl, "AlienCSI"@nl;
    <http://www.w3.org/ns/dcat#theme> <http://publications.europa.eu/resource/authority/data-theme/AGRI>, <http://publications.europa.eu/resource/authority/data-theme/ENVI>;
