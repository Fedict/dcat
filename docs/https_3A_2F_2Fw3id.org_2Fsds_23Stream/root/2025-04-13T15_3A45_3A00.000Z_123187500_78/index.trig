    <http://www.w3.org/ns/dcat#keyword> "afstand"@nl, "buurtparking"@nl, "park&rides mobiliteit"@nl, "autodeellocaties"@nl, "tijd"@nl, "publieke oplaadpalen"@nl, "autodelen"@nl, "bereik"@nl, "rotatieparkings"@nl, "buurtparkings"@nl, "bereikzone"@nl;
