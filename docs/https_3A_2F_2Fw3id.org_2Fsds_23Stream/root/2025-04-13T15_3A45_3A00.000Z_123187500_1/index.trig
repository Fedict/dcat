    <http://www.w3.org/ns/dcat#distribution> <http://data.gov.be/dist/biodiversity/495fd31a-9ac0-47fd-9ceb-e2dfbf2cfd91>;
    <http://www.w3.org/ns/dcat#contactPoint> _:b325_b324_b323_df_15_101;
    <http://purl.org/dc/terms/spatial> <https://sws.geonames.org/2802361/>.
_:b325_b324_b323_df_15_101 a <http://www.w3.org/2006/vcard/ns#Kind>;
    <http://www.w3.org/2006/vcard/ns#fn> "Francis Strobbe - Coordinator Belgian Species List | Belgian Bioarchaeological Inventory -  - +32 2 627 44 33";
    <http://www.w3.org/2006/vcard/ns#hasEmail> <mailto:francis.strobbe@naturalsciences.be>
