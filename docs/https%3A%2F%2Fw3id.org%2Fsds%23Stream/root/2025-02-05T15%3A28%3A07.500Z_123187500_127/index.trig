    <http://xmlns.com/foaf/0.1/Page> <http://walstat.iweps.be/fichiers/metadonnees/meta-215700.pdf>;
    <http://www.w3.org/ns/dcat#distribution> <http://opendata.iweps.be/api/data/json/234600_0/period=last>, <http://opendata.iweps.be/api/data/csv/234600_0/period=last>;
    <http://www.w3.org/ns/dcat#contactPoint> _:b38248_b38247_b38246_df_71_44327, _:b38248_b38247_b38246_df_71_44328;
