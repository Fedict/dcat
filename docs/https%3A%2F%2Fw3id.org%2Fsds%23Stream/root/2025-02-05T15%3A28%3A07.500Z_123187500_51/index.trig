    <http://www.w3.org/ns/dcat#keyword> "UvA-BiTS"@nl, "animal tracking"@nl, "Movebank"@nl, "altitude"@nl, "LifeWatch"@nl, "Observation"@nl, "frictionlessdata"@nl, "Occurrence"@nl, "animal movement"@nl, "biologging"@nl, "accelerometer"@nl, "gps tracking"@nl, "temperature"@nl, "birds"@nl;
    <http://www.w3.org/ns/dcat#theme> <http://publications.europa.eu/resource/authority/data-theme/AGRI>, <http://publications.europa.eu/resource/authority/data-theme/ENVI>;
