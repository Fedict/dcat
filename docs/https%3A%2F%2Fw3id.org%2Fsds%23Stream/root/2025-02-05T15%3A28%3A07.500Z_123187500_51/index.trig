    <http://www.w3.org/ns/dcat#keyword> "MVG"@nl, "AROHM"@nl, "KANALEN"@nl, "WATERLOPEN"@nl, "LANDSCHAPPEN"@nl, "MONUMENTEN"@nl, "STRUCTUURPLANNEN"@nl, "MVG - DEPARTEMENT LIN - AROHM - AFDELING MONUMENTE"@nl, "AGIV-PRODUCT"@nl, "AANVULLENDE INVENTARIS VAN DE RUIMTELIJKE LANDSCHA"@nl, "LANDSCHAPSPLANNING"@nl, "UITVOERINGSPLANNEN"@nl, "MER"@nl, "PUNTELM"@nl, "CLUVLAK"@nl, "CLUPUNT"@nl, "CLULIJN"@nl, "LANDSCHAPSKENMERKENKAART"@nl, "OpenbareMetadata"@nl, "lu_lkk_lle_wk"@nl;
