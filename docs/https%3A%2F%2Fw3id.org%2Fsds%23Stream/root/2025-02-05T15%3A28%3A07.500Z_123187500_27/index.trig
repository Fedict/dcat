    <http://www.w3.org/ns/dcat#keyword> "luchtkwaliteit"@nl, "seizoensoverlast"@nl, "vegetatie"@nl, "ozon"@nl, "Vlaanderen"@nl, "ozonoverschrijdingsindicator"@nl;
